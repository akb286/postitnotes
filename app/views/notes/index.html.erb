<h1>
  Post It Notes
</h1>
<p><%= link_to 'New Post It Note', new_note_path %></p>
<% @notes.each do |note| %>
  <h2><%= note.title %></h2>
  <%= link_to 'Show Post It Notes', note_path(note) %>
  <%= link_to 'Edit Note', edit_note_path(note) %>
  <%= link_to 'Delete Note', note, method: :delete %>
<% end %>